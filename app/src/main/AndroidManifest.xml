<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.muu.wonderfulchittagong">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ctg"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ctg"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomePage"
            android:label="Tourist Place in Chittagong">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".item"
            android:label="Details" />
        <activity android:name=".MainActivity" />
        <activity android:name=".market" />
        <activity android:name=".hotel" />
        <activity android:name=".emergency"></activity>
    </application>

</manifest>